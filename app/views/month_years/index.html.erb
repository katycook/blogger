<h1>Listing month_years</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @month_years.each do |month_year| %>
      <tr>
        <td><%= month_year.in_words %></td>
        <td><%= month_year.month %></td>
        <td><%= month_year.year %></td>
        <td><%= link_to 'Show', month_year %></td>
        <td><%= link_to 'Edit', edit_month_year_path(month_year) %></td>
        <td><%= link_to 'Destroy', month_year, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Month year', new_month_year_path %>
